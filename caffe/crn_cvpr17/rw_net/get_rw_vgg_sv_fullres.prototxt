  top: "vlad_core"
  param {
    name: "w_vladcore"
    lr_mult: 1
  }
  netvlad_param {
    cluster_num: 64
    local_dim: 512
    vlad_dim: 32768
  }
}
layer {
  name: "vlad_intra"
  type: "Intranorm"
  bottom: "vlad_core"
  top: "vlad_intra"
  intranorm_param {
    cluster_num: 64
  }
}
layer {
  name: "vlad_postL2"
  type: "Normalize"
  bottom: "vlad_intra"
  top: "vlad_postL2"
}