  }
}
####
layer {
  name: "rw_map/single"
  type: "Convolution"
  bottom: "concat_6"
  top: "rw_map/single"
  param {
    lr_mult: 0 #10
  }
  param {
    lr_mult: 10 #20
	decay_mult: 0
  }
  convolution_param {
    num_output: 1
    pad: 0
    kernel_size: 1
    stride: 1
    weight_filler {
      type: "constant"
      value: 1
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

